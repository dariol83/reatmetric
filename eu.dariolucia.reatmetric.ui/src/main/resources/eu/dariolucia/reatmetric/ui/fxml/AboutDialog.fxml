<?import javafx.scene.image.ImageView?>
<?import javafx.scene.web.WebView?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.dariolucia.reatmetric.ui.controller.AboutDialogController">
   <children>
      <ImageView fx:id="aboutImage" pickOnBounds="true" preserveRatio="true" VBox.vgrow="NEVER" />
      <WebView fx:id="aboutWebView" prefHeight="400.0" prefWidth="400.0" VBox.vgrow="NEVER" />
   </children>
</VBox>
