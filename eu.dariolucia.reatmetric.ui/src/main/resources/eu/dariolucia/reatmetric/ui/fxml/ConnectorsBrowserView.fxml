                  <Circle fx:id="serviceHealthStatus" fill="#003915" radius="8.0" stroke="WHITE" strokeType="INSIDE" />
               </items>
            </ToolBar>
            <ScrollPane fx:id="scrollpane" fitToWidth="true" VBox.vgrow="ALWAYS">
               <content>
                  <VBox fx:id="vbox" />
